{
  "name": "fetch-rest",
  "description": "Generic REST API client using Fetch API",
  "version": "1.0.0",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/RickWong/fetch-rest.git"
  },
  "homepage": "https://github.com/RickWong/fetch-rest",
  "keywords": [
    "fetch",
    "api",
    "rest",
    "restful",
    "promise"
  ],
  "main": "dist/lib/fetch-rest.js",
  "scripts": {
    "build": "concurrent --kill-others 'npm run build-full' 'npm run build-min'",
    "build-full": "webpack --verbose --colors --display-error-details --config webpack.umd.js src/lib/fetch-rest.js dist/lib/fetch-rest.js",
    "build-min": "webpack --verbose --colors --display-error-details --config webpack.umd.js -p src/lib/fetch-rest.js dist/lib/fetch-rest.min.js",
    "watch": "webpack --verbose --colors --display-error-details --config webpack.client-watch.js && webpack-dev-server --config webpack.client-watch.js",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "query-string": "3.0.0"
  },
  "devDependencies": {
    "babel": "6.1.18",
    "babel-core": "6.1.21",
    "babel-loader": "6.1.0",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-stage-0": "6.1.18",
    "concurrently": "0.1.1",
    "isomorphic-fetch": "2.2.0",
    "json-loader": "0.5.3",
    "react": "0.14.2",
    "react-dom": "0.14.2",
    "webpack": "1.12.6",
    "webpack-dev-server": "1.12.1"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
